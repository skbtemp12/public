
#!/bin/bash

# OSINTGRAM AUTOMATION

echo ""
echo ""
echo "\e[1;33m	Hello $USER, Welcome to, \e[0m"
echo ""
echo "\e[1;33m	OPEN-SOURCE INTELLIGENCE FOR  I N S T A G R A M \e[0m"
echo ""
echo ""

# Input Prompt

read -p "	Enter Target UserName or UserID: " ID
echo ""
echo "	Initiating... "
echo ""

# Initiating Osintgram

cd ~/.Osintgram ;
python3 main.py $ID;
exit
